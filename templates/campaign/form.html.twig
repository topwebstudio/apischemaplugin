{{ form_start(form) }}


<div class="row">

    <div class="col-md-10">
        <div class="form-group">
            <div class="row">
                <div class="col-md-6">
                    {{ form_row(form.campaignName, {'attr': {'placeholder': 'Campaign Name', 'class':'form-control'}}) }}
                </div>
                <div class="col-md-6">
                    {{ form_row(form.campaignId, {'attr': {'placeholder': 'Campaign ID', 'class':'form-control'}}) }}
                </div>


            </div>

            <div class="row">
                <div class="col-md-12">
                    {{ form_row(form.secretKey, {'attr': {'placeholder': 'Campaign Secret Key', 'class':'form-control'}}) }}
                </div>

            </div>
        </div>


    </div>


</div>


<button type="submit" class="btn btn-info btn-fill pull-right">Save</button>

<div class="clearfix"></div>

{{ form_end(form) }} 